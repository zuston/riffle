store_type = "MEMORY_LOCALFILE"
grpc_port = 21100
urpc_port = 22200
http_port = 19998
coordinator_quorum = ["uniffle-coordinator:21000"]
tags = ["riffle2", "datanode", "GRPC", "ss_v5", "GRPC_NETTY"]

[urpc_config]
get_index_rpc_version = 'V2'

[memory_store]
capacity = "2G"
dashmap_shard_amount = 128

[localfile_store]
data_paths = ["/tmp/riffle-server-1/data"]
healthy_check_min_disks = 0
disk_max_concurrency = 2000
disk_high_watermark = 1.0

[hybrid_store]
memory_spill_high_watermark = 0.5
memory_spill_low_watermark = 0.2
memory_spill_max_concurrency = 1000

[runtime_config]
read_thread_num = 20
write_thread_num = 50
grpc_thread_num = 50
http_thread_num = 5
default_thread_num = 10
dispatch_thread_num = 5

[metrics]
push_gateway_endpoint = "http://pushgateway:9091/"
push_interval_sec = 5
labels = { mode = "test" }

[log]
path = "/tmp/riffle-server-1/log"
rotation = "Never"
